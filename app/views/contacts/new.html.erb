

<div class="booyah-box col-10 offset-1">
 <%= simple_form_for @contact, url: contacts_path do |f| %>
        <div class="modal-body">
          <%= f.input :name %>
          <%= f.input :email %>
          <%= f.input :message %>
          <br />
          <%= f.submit "Send It", class: 'btn btn-primary' %>
      <% end %>

</div>



